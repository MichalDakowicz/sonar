{
  "rules": {
    "users": {
      "$uid": {
        "albums": {
          ".read": true,
          ".write": "$uid === auth.uid"
        },
        "history": {
          ".read": true,
          ".write": "$uid === auth.uid",
          ".indexOn": ["albumId", "timestamp"]
        }
      }
    }
  }
}
